{
    if(place_free(x-playerspeed,y))
    {
        x-=playerspeed ; 
        image_xscale = -abs(image_xscale) ; 
    }
    
    if(landed)
    {
        sprite_index = run_sprite ; 
    }
}


if(keys[UP_KEY] == 0)
{
    if(landed)
    {
        vspeed = 0 ; 
        vspeed -=jumpspeed ; 
        dj = true ; 
    }
    
    if(!landed &amp;&amp; dj &amp;&amp; vspeed &gt;=-1)
    {
        vspeed = 0 ; 
        vspeed -=jumpspeed ; 
        dj = false ;
    }
}

if(!landed)
{
    if(vspeed &gt;0) {sprite_index = fall_sprite ;} else{sprite_index = jump_sprite}
}


